{
  "name": "apify-actor-skcris",
  "version": "0.1.0",
  "private": true,
  "description": "Apify Actor that scrapes SKCRIS.sk",
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com>",
  "homepage": "https://github.com/jurooravec/apify-actor-skcris#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurooravec/apify-actor-skcris.git"
  },
  "bugs": {
    "url": "https://github.com/jurooravec/apify-actor-skcris/issues"
  },
  "license": "MIT",
  "keywords": [
    "apify",
    "actor",
    "skcris",
    "scraper"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/cjs/index.js",
    "start:dev": "ts-node ./src/index.ts",
    "build": "npm run build:cjs && npm run build:actor",
    "build:all": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:actor",
    "build:actor": "npx apify-actor-config gen -c ./dist/cjs/config.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "webpack",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "migrate": "npx apify-actor-utils migrate -d ./dist/cjs/actor-migrations/migrations",
    "unmigrate": "npx apify-actor-utils unmigrate -d ./dist/cjs/actor-migrations/migrations",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.5.1",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@sentry/node": "^7.50.0",
    "actor-spec": "^0.1.0",
    "apify": "^3.1.4",
    "apify-actor-config": "^1.0.2",
    "apify-actor-utils": "^0.8.6",
    "apify-client": "^2.7.1",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.3.1",
    "got": "^12.6.0",
    "joi": "^17.9.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "pkginfo": "^0.4.1",
    "playwright": "^1.32.3"
  }
}
